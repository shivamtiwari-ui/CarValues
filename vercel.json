{
    "version":2,
    "builds":[
        {
            "src":"mysite/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdSize":"15mb","runtime":"python3.12"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/statics/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"/mysite/wsgi.py"
        }
    ]
}